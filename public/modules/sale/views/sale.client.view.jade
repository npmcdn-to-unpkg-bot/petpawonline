			.setting: a(data-ui-sref='user.setting', title='Settings'): img(src='/images/icon-setting.png')
#content
	.contents
		.wrap(data-ui-view='content')
	.tabs(data-stick-to='{"item":"#header"}', data-stick-bar)
		.wrap
			ul
				li.item.active: a(data-ui-sref='sale.buy', title="buy pets"): img(src='/images/icon-buy.png')
				li.item: a(data-ui-sref='sale.sell', title="sell pets"): img(src='/images/icon-sell.png')
				li.item: a(data-ui-sref='sale.bid', title="bid pets"): img(src='/images/icon-bid.png')
				li.item: a(href='#search', title="search"): img(src='/images/icon-search.png')
				li.item: a(data-ui-sref='sale.information', title="information"): img(src='/images/icon-help.png')